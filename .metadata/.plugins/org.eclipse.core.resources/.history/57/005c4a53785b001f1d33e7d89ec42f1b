package ex02;

public class Q2 {

	public static void main(String[] args) {
		/*문제2
		 * 다섯명의 학생 cm점수를 키보드로 입력 받아서 출력하기
		 * -> 입력시에도 반복분
		 * ->출력시에도 반복문
		 */

		Scanner sc = new Scanner(System.in);
		
		System.out.println("컴퓨터 점수 >> ");
		int a;
		int[] nArr = new int[a];
				
		for(a=1; a<=5; a++);
		System.out.println(nArr[a]);
		
		int[] nArr1 = new int [5];
		
		int b;
		
		for(b=1; b<=5; b++);
		System.out.println(nArr1[b]);
	}

}
